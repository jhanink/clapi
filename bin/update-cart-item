. ./include/script-include

if [ $DAS_has_params == 1 ]
then
  CART_ID=`./get-cart --EVAL=cart.id`
  node $DAS_index --name=$DAS_script_name --cartId=$CART_ID --itemId=$1 --quantity=$2 --WRAP > $DAS_output_dir/TEMP_RESULT.json
  DAS_handle_response;
  cp $DAS_output_dir/$DAS_script_name.json $DAS_output_dir/LATEST_CART.json
fi

DAS_print_output $DAS_output_dir/LATEST_CART.json;